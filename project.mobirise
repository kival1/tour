ndition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.4\" condition=\"overlay && bg.type !== 'color'\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\"></div>\n    <div class=\"two_background\"></div>\n    \n        <div class mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        \n        <div class=\"row justify-content-center\">\n            <div class=\"col-12 col-lg-{{textWidth}}\">\n                <div class=\"box\">\n                    <h4 class=\"mbr-section-title mbr-fonts-style\" data-app-selector=\".mbr-section-title\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\">View in Action</h4>\n                    <div class=\"mbr-media show-modal align-center\" data-modal=\".modalWindow\">\n                        <a href=\"https://drive.google.com/file/d/1rDtWs-c86kswzTBTeSwcBvbtNGd1R4t6/view?usp=sharing\"><img src=\"@PROJECT_PATH@/assets/images/%202023-04-24%20at%203.44.52%20pm-1256x709.png\" alt=\"Vedeo\"></a>\n                        <div class=\"circle\"></div>\n                        <div class=\"iconfont-wrapper\">\n                            <a href=\"https://drive.google.com/file/d/1rDtWs-c86kswzTBTeSwcBvbtNGd1R4t6/view?usp=sharing\"><span mbr-icon class=\"mbr-iconfont mobi-mbri-play mobi-mbri\"></span></a>\n                        </div>\n                        <div class=\"shape\" mbr-if=\"showShape\">\n                            <img src=\"@ADDONS_PATH@/bistrom5/components/_images/shape.png\" alt=\"Mobirise\">\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n    <div>\n        <div class=\"modalWindow\" style=\"display: none;\">\n            <div class=\"modalWindow-container\">\n                <div class=\"modalWindow-video-container\">\n                    \n                    \n                </div>\n            </div>\n        </div>\n    </div>\n</section>",
